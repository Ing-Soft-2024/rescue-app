{
  "expo": {
    "name": "rescueapp",
    "slug": "rescueapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "owner": "rescue",
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDkO9yZTw1mmEBrc5Vy6mIO1e5gAUfbG94"
      },
      "NSLocationWhenInUseUsageDescription": "Esta aplicaci贸n necesita acceder a tu ubicaci贸n para mostrarte el mapa.",
      "NSLocationAlwaysUsageDescription": "Esta aplicaci贸n necesita acceder a tu ubicaci贸n en segundo plano.",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ]
      },
      "bundleIdentifier": "com.kepleris.rescue"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.kepleris.rescue",
        "android": {
          "config": {
            "googleMaps": {
              "apiKey": "AIzaSyDkO9yZTw1mmEBrc5Vy6mIO1e5gAUfbG94"
            }
          }
        }
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.kepleris.rescue",
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "./src/plugins/maps"
      ],
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e35c2fa1-6173-4938-b9d1-0742c07fb037"
      }
    }
  }
}